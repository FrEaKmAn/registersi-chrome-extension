{
  "manifest_version": 2,

  "name": "register.si WHOIS",
  "description": "Easily get whois information for .si domains",
  "version": "1.1",

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://www.register.si/"
  ]
}
